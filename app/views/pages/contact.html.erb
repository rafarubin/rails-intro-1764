<h2>Contact</h2>

<% members = %w[andres antonio jorge manuel] %>

<ol>
  <% members.each do |member| %>
    <li>
      <%= member.capitalize %>
    </li>
  <% end %>
</ol>

<form action="/contact" method="get">
  <input type="text" name="member" placeholder="Who are you looking for?">
  <input type="submit">
</form>


<%= link_to 'Go home', root_path %>

<% console %>
